keywords = {Built environment,Health behaviors,Health disparities,Neighborhood,New Haven, Connecticut, USA,Social environment,Socioeconomic status},
pages = {106--114},
pmid = {23642646},
publisher = {Elsevier Ltd},
title = {{Disentangling neighborhood contextual associations with child body mass index, diet, and physical activity: The role of built, socioeconomic, and social environments}},
