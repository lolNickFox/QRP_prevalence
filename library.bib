title = {{The Fallacy of Placing Confidence in Confidence Intervals}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3403681{\%}7B{\&}{\%}7Dtool=pmcentrez{\%}7B{\&}{\%}7Drendertype=abstract},
