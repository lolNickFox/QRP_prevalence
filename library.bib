journal = {Psychological bulletin},
keywords = {cycle shifts in women,ovulation},
number = {5},
pages = {1272--1280},
pmid = {25180805},
title = {{The garden of forking paths: Why multiple comparisons can be a problem, even when there is no “fishing expedition” or “p-hacking” and the research hypothesis was posited ahead of time}},
url = {http://www.stat.columbia.edu/{~}gelman/research/unpublished/p{\_}hacking.pdf{\%}5Cnhttp://doi.apa.org/getdoi.cfm?doi=10.1037/a0037714},
