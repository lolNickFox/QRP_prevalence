issn = {00028223},
number = {8},
pages = {1105--1118},
pmid = {12171455},
title = {{Dietary acculturation: Applications to nutrition research and dietetics}},
url = {http://www.sciencedirect.com/science/article/pii/S0002822302902476},
volume = {102},
year = {2002}
}
