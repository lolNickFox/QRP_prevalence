file = {:Users/Nick{\_}Fox/Dropbox/Nick/papers/Wirtz 2016 RDS comparison hiv prevalence in MSM.pdf:pdf},
issn = {19326203},
journal = {PLoS ONE},
number = {6},
pages = {1--15},
pmid = {27248818},
title = {{Comparison of respondent driven sampling estimators to determine HIV prevalence and population characteristics among men who have sex with men in Moscow, Russia}},
volume = {11},
year = {2016}
}
