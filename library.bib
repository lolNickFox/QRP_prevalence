file = {:Users/Nick{\_}Fox/Library/Application Support/Mendeley Desktop/Downloaded/Nosek, Spies, Motyl - 2012 - Scientific Utopia II. Restructuring Incentives and Practices to Promote Truth Over Publishability.pdf:pdf},
isbn = {1745-6916, 1745-6924},
issn = {1745-6916},
journal = {Perspectives on Psychological Science},
keywords = {false positives,incentives,methodology,motivated reasoning,replication},
number = {6},
pages = {615--631},
