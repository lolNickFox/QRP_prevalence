journal = {Health care analysis : HCA : journal of health philosophy and policy},
keywords = {Autistic Disorder,Autistic Disorder: psychology,Civil Rights,Culture,Diagnostic and Statistical Manual of Mental Disord,Human Rights,Humans,Vulnerable Populations},
month = {mar},
